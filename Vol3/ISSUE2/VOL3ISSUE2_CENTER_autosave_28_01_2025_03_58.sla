            <StoryText>
                <DefaultStyle LINESP="10" FONTSIZE="9"/>
                <ITEXT FONT="Bell MT Bold" FONTSIZE="20" CH="To My 8 A.M.s:"/>
                <para ALIGN="1" LINESP="20"/>
                <ITEXT FONT="Bell MT Italic" CH="Alex Werth"/>
                <para ALIGN="1" LINESP="10"/>
                <para LINESP="10"/>
                <ITEXT FONT="Bell MT Regular" CH="Up! We are Up!"/>
                <para LINESP="10"/>
                <ITEXT FONT="Bell MT Regular" CH="I cheer myself to rise"/>
                <para LINESP="10"/>
                <ITEXT FONT="Bell MT Regular" CH="At the crack of seven-twenty,"/>
                <para LINESP="10"/>
                <ITEXT FONT="Bell MT Regular" CH="‘Fore the sun has hit the skies,"/>
                <para LINESP="10"/>
                <para LINESP="10"/>
                <ITEXT FONT="Bell MT Regular" CH="On a wonderous new Monday,"/>
                <para LINESP="10"/>
                <ITEXT FONT="Bell MT Regular" CH="I’m triumphantly awake"/>
                <para LINESP="10"/>
                <ITEXT FONT="Bell MT Regular" CH="In sheer elation for the 8am that I "/>
                <para LINESP="10"/>
                <ITEXT FONT="Bell MT Regular" CH="– with no especially strong feelings –"/>
                <para LINESP="10"/>
                <ITEXT FONT="Bell MT Regular" CH="Take."/>
                <para LINESP="10"/>
                <para LINESP="10"/>
                <ITEXT FONT="Bell MT Regular" CH="A truly magical day awaits, that starts at lovely number 8."/>
                <para LINESP="10"/>
                <para LINESP="10"/>
                <ITEXT FONT="Bell MT Regular" CH="Up! We are Up!"/>
                <para LINESP="10"/>
                <ITEXT FONT="Bell MT Regular" CH="I beg myself to rise"/>
                <para LINESP="10"/>
                <ITEXT FONT="Bell MT Regular" CH="At the hour of seven-forty,"/>
                <para LINESP="10"/>
                <ITEXT FONT="Bell MT Regular" CH="Peeling crust from out my eyes,"/>
                <para LINESP="10"/>
                <para LINESP="10"/>
                <ITEXT FONT="Bell MT Regular" CH="On a magical new Wednesday"/>
                <para LINESP="10"/>
                <ITEXT FONT="Bell MT Regular" CH="I have once again arose"/>
                <para LINESP="10"/>
                <ITEXT FONT="Bell MT Regular" CH="For an 8 am that I, against all reasoning, I chose."/>
                <para LINESP="10"/>
                <para LINESP="10"/>
                <ITEXT FONT="Bell MT Regular" CH="A sorry start turned out okay – at least it started at 8 today."/>
                <para LINESP="10"/>
                <para LINESP="10"/>
                <para LINESP="10"/>
                <ITEXT FONT="Bell MT Regular" FONTSIZE="30" CH="Fuck!"/>
                <para LINESP="10"/>
                <para LINESP="10"/>
                <ITEXT FONT="Bell MT Regular" CH="I am up, I am up,"/>
                <para LINESP="10"/>
                <ITEXT FONT="Bell MT Regular" CH="I have no idea the time"/>
                <para LINESP="10"/>
                <ITEXT FONT="Bell MT Regular" CH="I’ve stumbled from a cloud of dreams"/>
                <para LINESP="10"/>
                <ITEXT FONT="Bell MT Regular" CH="Into this lumpy bed of mine"/>
                <para LINESP="10"/>
                <para LINESP="10"/>
                <ITEXT FONT="Bell MT Regular" CH="And "/>
                <ITEXT FONT="Bell MT Regular" FONTSIZE="15" CH="fuck!"/>
                <ITEXT FONT="Bell MT Regular" CH=" Oh "/>
                <ITEXT FONT="Bell MT Regular" FONTSIZE="25" CH="fuck!"/>
                <para LINESP="10"/>
                <ITEXT FONT="Bell MT Regular" CH="It is well beyond the time"/>
                <para LINESP="10"/>
                <ITEXT FONT="Bell MT Regular" CH="When I should’ve shambled out to class"/>
                <para LINESP="10"/>
                <ITEXT FONT="Bell MT Regular" CH="An hour ago from 9!"/>
                <para LINESP="10"/>
                <para LINESP="10"/>
                <ITEXT FONT="Bell MT Regular" CH="But…"/>
                <para LINESP="10"/>
                <para LINESP="10"/>
                <ITEXT FONT="Bell MT Regular" CH="Before I leave my sunny dorm,"/>
                <para LINESP="10"/>
                <ITEXT FONT="Bell MT Regular" CH="I’ll take a little rest."/>
                <para LINESP="10"/>
                <ITEXT FONT="Bell MT Regular" CH="A half an hour all my own,"/>
                <para LINESP="10"/>
                <ITEXT FONT="Bell MT Regular" CH="And I am at my best."/>
                <para LINESP="10"/>
                <para LINESP="10"/>
                <para LINESP="10"/>
                <para LINESP="10"/>
                <para LINESP="10"/>
            </StoryText>
        </PAGEOBJECT>
