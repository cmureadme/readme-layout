        <PAGEOBJECT XPOS="-601.35" YPOS="838.651968503937" OwnPage="-1" ItemID="398649264" PTYPE="4" WIDTH="154.5" HEIGHT="424.15" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="7.2" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L154.5 0 L154.5 424.15 L0 424.15 L0 0 Z" copath="M0 0 L154.5 0 L154.5 424.15 L0 424.15 L0 0 Z" gXpos="-601.35" gYpos="838.651968503937" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle LINESP="10" FONTSIZE="9"/>
                <ITEXT FONT="Bell MT Bold" FONTSIZE="20" CH="A Song for Dear Ol' Tech"/>
                <para ALIGN="1" LINESP="20"/>
                <ITEXT FONT="Bell MT Italic" FONTSIZE="9" CH="To the tune of &quot;A Model of a Modern Major General"/>
                <ITEXT CH="&quot;, "/>
                <ITEXT FONT="Bell MT Italic" CH="by Gilbert and Sullivan"/>
                <para ALIGN="1" LINESP="10"/>
                <para ALIGN="1" LINESP="10"/>
                <ITEXT FONT="Bell MT Regular" CH="I am the very model of a student here at Carnegie,"/>
                <para ALIGN="1"/>
                <ITEXT FONT="Bell MT Regular" CH="With plans to triple major in AI, CS, and ECE"/>
                <para ALIGN="1"/>
                <ITEXT FONT="Bell MT Regular" CH="I've memorized the answer key to every leetcode medium"/>
                <para ALIGN="1"/>
                <ITEXT FONT="Bell MT Regular" CH="I spend my time on Stack Exchange to mitigate the tedium"/>
                <para ALIGN="1"/>
                <para ALIGN="1"/>
                <ITEXT FONT="Bell MT Regular" CH="I’ve crashed in classrooms everywhere from Gates to Wean to Hamerschlag"/>
                <para ALIGN="1"/>
                <ITEXT FONT="Bell MT Regular" CH="Complained about insomnia with Red Bull in my sleeping bag"/>
                <para ALIGN="1"/>
                <ITEXT FONT="Bell MT Regular" CH="I try to brag but sometimes the imposter syndrome gets to me"/>
                <para ALIGN="1"/>
                <ITEXT FONT="Bell MT Regular" CH="I'm pretty sure that half this school still wants to go to MIT"/>
                <para ALIGN="1"/>
                <para ALIGN="1"/>
                <ITEXT FONT="Bell MT Regular" CH="But that's okay I know that I'm an academic superstar"/>
                <para ALIGN="1"/>
                <ITEXT FONT="Bell MT Regular" CH="I've only bombed two tests, a quiz, a midterm, and a lab so far"/>
                <para ALIGN="1"/>
                <ITEXT FONT="Bell MT Regular" CH="I'll try to make a comeback when my mid semester grades come out"/>
                <para ALIGN="1"/>
                <ITEXT FONT="Bell MT Regular" CH="I've got a homework due at two but I don't know what that's about"/>
                <para ALIGN="1"/>
                <para ALIGN="1"/>
                <ITEXT FONT="Bell MT Regular" CH="I’m unsure what career of mediocrity will be my fate"/>
                <para ALIGN="1"/>
                <ITEXT FONT="Bell MT Regular" CH="But at this point dear god above, oh please just let me graduate"/>
                <para ALIGN="1"/>
                <ITEXT FONT="Bell MT Regular" CH="And thus with plans to triple major: AI, CS, ECE"/>
                <para ALIGN="1"/>
                <ITEXT FONT="Bell MT Regular" CH="I am the very model of a student here at Carnegie. "/>
                <para ALIGN="1"/>
            </StoryText>
        </PAGEOBJECT>
