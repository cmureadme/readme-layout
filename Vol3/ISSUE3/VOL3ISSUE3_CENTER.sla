        <PAGEOBJECT XPOS="481.629286809816" YPOS="536.051968503938" OwnPage="0" ItemID="258837360" PTYPE="4" WIDTH="207.821180981595" HEIGHT="245.696497753731" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="7.2" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L207.821 0 L207.821 245.696 L0 245.696 L0 0 Z" copath="M0 0 L207.821 0 L207.821 245.696 L0 245.696 L0 0 Z" gXpos="481.629286809816" gYpos="-1127.94803149607" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle LINESP="10" FONT="Bell MT Regular" FONTSIZE="9"/>
                <ITEXT FONT="Bell MT Bold" CH="Bridget McMahon "/>
                <para ALIGN="1"/>
                <ITEXT FONT="Bell MT Italic" CH="One of the six quarks"/>
                <para ALIGN="1"/>
                <para/>
                <ITEXT FONT="Bell MT Bold" CH="10. Bob Ross"/>
                <para/>
                <ITEXT CH="    “Just beat the devil out of it” was NOT about the paintbrush."/>
                <para/>
                <ITEXT FONT="Bell MT Bold" CH="9. Thom Yorke"/>
                <para/>
                <ITEXT CH="    He wrote Creep."/>
                <para/>
                <ITEXT FONT="Bell MT Bold" CH="8. Ellen"/>
                <para/>
                <ITEXT CH="    She’ll abuse you just like how she abused her staff."/>
                <para/>
                <ITEXT FONT="Bell MT Bold" CH="7. Meryl Streep"/>
                <para/>
                <ITEXT CH="    Have you seen The Devil Wears Prada?"/>
                <para/>
                <ITEXT FONT="Bell MT Bold" CH="6. Michael Cera"/>
                <para/>
                <ITEXT CH="    &gt;:)"/>
                <para/>
                <ITEXT FONT="Bell MT Bold" CH="5. Stanley Tucci"/>
                <para/>
                <ITEXT CH="    The Italian Stallion."/>
                <para/>
                <ITEXT FONT="Bell MT Bold" CH="4. Michelle Obama"/>
                <para/>
                <ITEXT CH="    She'll ruin you just like she ruined school lunch."/>
                <para/>
                <ITEXT FONT="Bell MT Bold" CH="3. Martha Stewart"/>
                <para/>
                <ITEXT CH="    That time in prison changed her."/>
                <para/>
