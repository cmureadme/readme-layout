        <PAGEOBJECT XPOS="419" YPOS="1778.00196850394" OwnPage="2" ItemID="296100848" PTYPE="4" WIDTH="278.6" HEIGHT="271.649999999997" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="2" COLGAP="7.2" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L278.6 0 L278.6 271.65 L0 271.65 L0 0 Z" copath="M0 0 L278.6 0 L278.6 271.65 L0 271.65 L0 0 Z" gXpos="419" gYpos="946.001968503937" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle LINESP="10" FONT="Bell MT Regular" FONTSIZE="9"/>
                <ITEXT FONT="Bell MT Bold" CH="Maximillian Bartholemow VIII"/>
                <para ALIGN="1" LINESP="10"/>
                <ITEXT FONT="Bell MT Bold" CH=" "/>
                <ITEXT FONT="Bell MT Italic" CH="Distinguished Gentleman, Speaking Truth to Power"/>
                <para ALIGN="1" LINESP="10"/>
                <para ALIGN="1" LINESP="10"/>
                <ITEXT FONT="Bell MT Regular" FONTSIZE="9" SCALEH="100" SCALEV="100" CH="   Let it become beknownst to whomstever accused our collective of various “allegations”, that they are all factually incoherent and blatantly untrue. This response will be organized in the following format — an untrue statement made by an adversary, and our rebuttal."/>
                <para/>
                <para/>
                <ITEXT FONT="Bell MT Bold" FONTSIZE="9" SCALEH="100" SCALEV="100" CH="“Readme will be responsible for various cases of food poisoning on campus on April 1st, 2024”"/>
                <para/>
                <para/>
                <ITEXT FONT="Bell MT Regular" FONTSIZE="9" SCALEH="100" SCALEV="100" CH="We are unaware of which individual would provide you with such abhorrent misinformation."/>
                <para/>
                <para/>
                <ITEXT FONT="Bell MT Bold" FONTSIZE="9" SCALEH="100" SCALEV="100" CH="“Readme likes to place banana peels all over campus so that people will slip and die”"/>
                <para/>
                <para/>
                <ITEXT FONT="Bell MT Regular" FONTSIZE="9" SCALEH="100" SCALEV="100" CH="We do not dabble in the realm of banana peels — orange peels are our domain."/>
                <para/>
                <para/>
                <ITEXT FONT="Bell MT Bold" FONTSIZE="9" SCALEH="100" SCALEV="100" CH="“Readme made an improvised weapon of mass destruction and currently holds it in CUC 306”"/>
                <para/>
                <para/>
                <ITEXT FONT="Bell MT Regular" FONTSIZE="9" SCALEH="100" SCALEV="100" CH="What an insult to our reputation! I can assure you that significant research and efforts were put into the fabrication of such a device!"/>
                <ITEXT CH=" No improvisation was done in any way, shape, or form."/>
                <para/>
                <para/>
                <ITEXT FONT="Bell MT Bold" FONTSIZE="9" SCALEH="100" SCALEV="100" CH="“Readme destroyed the Donner reading room”"/>
                <para/>
                <para/>
                <ITEXT FONT="Bell MT Regular" FONTSIZE="9" SCALEH="100" SCALEV="100" CH="The assertions that we burned down the Donner reading room are, much like Donner, baseless and offensive. We destroyed the entirety of that wretched building."/>
                <para/>
                <para/>
                <ITEXT FONT="Bell MT Bold" FONTSIZE="9" SCALEH="100" SCALEV="100" CH="“Eshaan Joshi does not pay his workers”"/>
                <para/>
                <para/>
                <ITEXT FONT="Bell MT Regular" FONTSIZE="9" SCALEH="100" SCALEV="100" CH="I admit defeat, this may have at least a small hint of truthfulness."/>
                <para/>
            </StoryText>
        </PAGEOBJECT>
