        <PAGEOBJECT XPOS="116.489908256881" YPOS="1864.48" OwnPage="2" ItemID="241795440" PTYPE="4" WIDTH="283.5" HEIGHT="66.1219685039362" FRTYPE="0" CLIPEDIT="1" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L283.5 0 L283.5 66.122 L0 66.122 L0 0 Z" copath="M0 0 L283.5 0 L283.5 66.122 L0 66.122 L0 0 Z" gXpos="116.489908256881" gYpos="1864.48" gWidth="283.5" gHeight="74.5626435464906" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
