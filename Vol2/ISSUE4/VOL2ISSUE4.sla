        <PAGEOBJECT XPOS="423.220183486239" YPOS="3056.1871559633" OwnPage="3" ItemID="298208416" PTYPE="4" WIDTH="281.25" HEIGHT="65.8398125406338" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L281.25 0 L281.25 65.8398 L0 65.8398 L0 0 Z" copath="M0 0 L281.25 0 L281.25 65.8398 L0 65.8398 L0 0 Z" gXpos="288.715802752294" gYpos="1349.6" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
