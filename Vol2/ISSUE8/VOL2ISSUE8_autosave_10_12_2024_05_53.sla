                <para PARENT="Default Paragraph Style" ALIGN="1" LINESP="9.5" ParagraphEffectOffset="0" ParagraphEffectIndent="0" DROP="0" DROPLIN="2" Bullet="0" BulletStr="•" Numeration="0" NumerationFormat="0" NumerationName="&lt;local block&gt;" NumerationLevel="0" NumerationPrefix="" NumerationSuffix="." NumerationStart="1" NumerationRestart="2" NumerationOther="1" NumerationHigher="1" MinWordTrack="1"/>
