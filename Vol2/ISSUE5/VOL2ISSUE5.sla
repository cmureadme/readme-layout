        <PAGEOBJECT XPOS="116.825688073395" YPOS="953.132243733301" OwnPage="1" ItemID="237430096" PTYPE="4" WIDTH="280.85" HEIGHT="93.1249999999967" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L280.85 0 L280.85 93.125 L0 93.125 L0 0 Z" copath="M0 0 L280.85 0 L280.85 93.125 L0 93.125 L0 0 Z" gXpos="760.75" gYpos="831.301968503931" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
