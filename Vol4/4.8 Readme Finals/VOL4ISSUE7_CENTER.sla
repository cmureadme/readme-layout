                <ITEXT CH="Carnegie cracks down on Crystal Math"/>
                <para ALIGN="1" LINESP="28"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="-395.9375" YPOS="888.30196850394" OwnPage="-1" ItemID="190525280" PTYPE="4" WIDTH="457.9375" HEIGHT="62.07303149606" FRTYPE="0" CLIPEDIT="1" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L457.938 0 L457.938 62.073 L0 62.073 L0 0 Z" copath="M0 0 L457.936 0 L457.936 62.0731 L0 62.0731 L0 0 Z" gXpos="-395.9375" gYpos="1720.30196850394" gWidth="282.55" gHeight="45.4146798798563" ALIGN="1" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
