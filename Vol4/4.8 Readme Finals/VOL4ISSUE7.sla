        <PAGEOBJECT XPOS="112" YPOS="2525.88946850394" OwnPage="3" ItemID="197336544" PTYPE="4" WIDTH="246.375" HEIGHT="121.91053149606" FRTYPE="0" CLIPEDIT="1" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L246.375 0 L246.375 121.911 L0 121.911 L0 0 Z" copath="M0 0 L246.375 0 L246.375 121.911 L0 121.911 L0 0 Z" gXpos="112" gYpos="1693.88946850394" gWidth="282.55" gHeight="45.4146798798563" ALIGN="1" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
