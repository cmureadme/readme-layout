        <PAGEOBJECT XPOS="115.909744094488" YPOS="863.11850393701" OwnPage="1" ItemID="198746240" PTYPE="4" WIDTH="266.3125" HEIGHT="119.167210348704" FRTYPE="0" CLIPEDIT="1" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L266.313 0 L266.313 119.167 L0 119.167 L0 0 Z" copath="M0 0 L266.313 0 L266.313 119.168 L0 119.168 L0 0 Z" gXpos="115.909744094488" gYpos="863.11850393701" gWidth="282.55" gHeight="45.4146798798563" ALIGN="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
