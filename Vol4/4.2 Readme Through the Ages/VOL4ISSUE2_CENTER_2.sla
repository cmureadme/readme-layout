        <PAGEOBJECT XPOS="111.88474409449" YPOS="28.11850393701" OwnPage="0" ItemID="194505584" PTYPE="4" WIDTH="283.2" HEIGHT="61.9959645669268" FRTYPE="0" CLIPEDIT="1" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L283.2 0 L283.2 61.996 L0 61.996 L0 0 Z" copath="M0 0 L283.2 0 L283.2 61.9963 L0 61.9963 L0 0 Z" gXpos="111.88474409449" gYpos="860.11850393701" gWidth="282.55" gHeight="45.4146798798563" ALIGN="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
