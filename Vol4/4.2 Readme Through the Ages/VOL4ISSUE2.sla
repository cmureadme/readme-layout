                <DefaultStyle ALIGN="1" LINESP="28" FONT="Special Elite Regular" FONTSIZE="33" SCALEH="90" KERN="-5"/>
                <ITEXT CH="Fruity Take on CMU Housing"/>
                <trail ALIGN="1"/>
